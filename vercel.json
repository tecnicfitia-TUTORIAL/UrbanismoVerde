{
  "buildCommand": "npm --prefix frontend install && npm --prefix frontend run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "echo 'Skip root install'",
  "functions": {
    "api/analyze.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/analyze",
      "destination": "/api/analyze.py"
    }
  ]
}
